    "import cv2\n",
    "import time\n",
    "import os\n",
    "from datetime import datetime\n",
    "\n",
    "# RTSP stream URL\n",
    "rtsp_url = \"rtsp://admin:******"\n",
    "\n",
    "# Output directory\n",
    "output_dir = \"saved_chunks\"\n",
    "os.makedirs(output_dir, exist_ok=True)\n",
    "\n",
    "# Chunk duration in seconds (2 minutes)\n",
    "chunk_duration = 2 * 60\n",
    "max_retries = 5  # Max reconnection attempts\n",
    "\n",
    "def get_output_filename(chunk_number):\n",
    "    timestamp = datetime.now().strftime('%Y%m%d_%H%M%S')\n",
    "    return os.path.join(output_dir, f\"output_chunk_{chunk_number}_{timestamp}.mp4\")\n",
    "\n",
    "def should_continue(chunk_number):\n",
    "    choice = input(f\"\\n✅ Finished chunk #{chunk_number}. Do you want to continue recording? (y/n): \").strip().lower()\n",
    "    return choice == 'y'\n",
    "\n",
    "def open_stream(retries=3):\n",
    "    for attempt in range(retries):\n",
    "        cap = cv2.VideoCapture(rtsp_url, cv2.CAP_FFMPEG)\n",
    "        if cap.isOpened():\n",
    "            print(f\"🔌 Connected to stream (attempt {attempt + 1})\")\n",
    "            return cap\n",
    "        print(f\"⚠️ Failed to connect (attempt {attempt + 1}/{retries})\")\n",
    "        time.sleep(2)\n",
    "    print(\"❌ Unable to connect to the RTSP stream after multiple attempts.\")\n",
    "    return None\n",
    "\n",
    "# Initialize stream\n",
    "cap = open_stream(max_retries)\n",
    "if cap is None:\n",
    "    exit()\n",
    "\n",
    "# Get stream properties\n",
    "fps = cap.get(cv2.CAP_PROP_FPS) or 25\n",
    "frame_width = int(cap.get(cv2.CAP_PROP_FRAME_WIDTH))\n",
    "frame_height = int(cap.get(cv2.CAP_PROP_FRAME_HEIGHT))\n",
    "fourcc = cv2.VideoWriter_fourcc(*'mp4v')\n",
    "\n",
    "chunk_number = 1\n",
    "start_time = time.time()\n",
    "out = cv2.VideoWriter(get_output_filename(chunk_number), fourcc, fps, (frame_width, frame_height))\n",
    "\n",
    "print(f\"\\n🎥 Recording started (headless) - Chunk #{chunk_number}\")\n",
    "\n",
    "try:\n",
    "    while True:\n",
    "        ret, frame = cap.read()\n",
    "\n",
    "        if not ret:\n",
    "            print(\"⚠️ Frame grab failed. Attempting reconnection...\")\n",
    "            cap.release()\n",
    "            cap = open_stream(max_retries)\n",
    "            if cap is None:\n",
    "                break\n",
    "            continue\n",
    "\n",
    "        out.write(frame)\n",
    "\n",
    "        if time.time() - start_time >= chunk_duration:\n",
    "            out.release()\n",
    "            if not should_continue(chunk_number):\n",
    "                print(\"🛑 Stopping recording.\")\n",
    "                break\n",
    "            chunk_number += 1\n",
    "            out = cv2.VideoWriter(get_output_filename(chunk_number), fourcc, fps, (frame_width, frame_height))\n",
    "            start_time = time.time()\n",
    "            print(f\"\\n🎬 Started new chunk #{chunk_number}\")\n",
    "\n",
    "except KeyboardInterrupt:\n",
    "    print(\"\\n🛑 Interrupted by user.\")\n",
    "\n",
    "# Cleanup\n",
    "cap.release()\n",
    "out.release()\n",
    "print(\"✅ Recording complete and resources released.\")\n"
   ]
