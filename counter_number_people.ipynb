    "import cv2\n",
    "\n",
    "# RTSP stream URL\n",
    "rtsp_url = \"rtsp://admin:**********"\n",
    "\n",
    "# Desired resolution (Full HD)\n",
    "desired_width = 1920\n",
    "desired_height = 1080\n",
    "\n",
    "# Open the RTSP stream using FFmpeg backend for better compatibility\n",
    "cap = cv2.VideoCapture(rtsp_url, cv2.CAP_FFMPEG)\n",
    "\n",
    "# Set resolution (will only work if the camera supports it)\n",
    "cap.set(cv2.CAP_PROP_FRAME_WIDTH, desired_width)\n",
    "cap.set(cv2.CAP_PROP_FRAME_HEIGHT, desired_height)\n",
    "\n",
    "if not cap.isOpened():\n",
    "    print(\"❌ Failed to connect to the RTSP stream.\")\n",
    "    exit()\n",
    "\n",
    "print(\"✅ Streaming RTSP in HD... Press 'q' to quit.\")\n",
    "\n",
    "while True:\n",
    "    ret, frame = cap.read()\n",
    "    if not ret:\n",
    "        print(\"⚠️ Lost connection. Reconnecting...\")\n",
    "        cap.release()\n",
    "        cap = cv2.VideoCapture(rtsp_url, cv2.CAP_FFMPEG)\n",
    "        continue\n",
    "\n",
    "    cv2.imshow(\"RTSP Stream [HD]\", frame)\n",
    "\n",
    "    if cv2.waitKey(1) & 0xFF == ord('q'):\n",
    "        break\n",
    "\n",
    "cap.release()\n",
    "cv2.destroyAllWindows()\n"
