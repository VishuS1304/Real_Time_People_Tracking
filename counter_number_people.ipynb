    "import cv2\n",
    "import numpy as np\n",
    "from ultralytics import YOLO\n",
    "from deep_sort_realtime.deepsort_tracker import DeepSort\n",
    "from collections import defaultdict\n",
    "import csv\n",
    "import time\n",
    "from datetime import datetime\n",
    "import os\n",
    "\n",
    "# === Load YOLOv8 model with FP16 optimization ===\n",
    "model = YOLO(\"best3.pt\")\n",
    "model.fuse()  # Fuse Conv2d layers for speed\n",
    "model.half()  # Use half precision (FP16)\n",
    "\n",
    "# === Load Video ===\n",
    "cap = cv2.VideoCapture(\"saved_chunks/output_chunk_1_20250418_173511.mp4\")\n",
    "ret, first_frame = cap.read()\n",
    "if not ret:\n",
    "    print(\"❌ Could not read video.\")\n",
    "    exit()\n",
    "\n",
    "height, width = first_frame.shape[:2]\n",
    "is_horizontal = width >= height\n",
    "display_size = (960, int(960 * height / width)) if is_horizontal else (int(960 * width / height), 960)\n",
    "\n",
    "# === ROI Polygon and Mask ===\n",
    "roi_points = np.array([\n",
    "    (806, 234), (934, 232), (960, 268), (1027, 262), (1109, 369),\n",
    "    (1173, 458), (1152, 640), (821, 681), (818, 534), (812, 333)\n",
    "], dtype=np.int32)\n",
    "roi_mask = np.zeros((height, width), dtype=np.uint8)\n",
    "cv2.fillPoly(roi_mask, [roi_points], 1)\n",
    "\n",
    "# === Entry and Exit Line Points ===\n",
    "entry_line = [(806, 234), (940, 232)]\n",
    "exit_line = [(1155, 432), (816, 428)]\n",
    "line_color = (0, 0, 0)  # Purple\n",
    "\n",
    "def get_line_vector(p1, p2):\n",
    "    return np.array([p2[0] - p1[0], p2[1] - p1[1]])\n",
    "\n",
    "def direction_sign(p1, p2, curr):\n",
    "    a = get_line_vector(p1, p2)\n",
    "    b = np.array([curr[0] - p1[0], curr[1] - p1[1]])\n",
    "    return np.sign(a[0] * b[1] - a[1] * b[0])\n",
    "\n",
    "# === DeepSORT Tracker ===\n",
    "tracker = DeepSort(max_age=30)\n",
    "\n",
    "# === Output Video Writer ===\n",
    "out = cv2.VideoWriter(\n",
    "    \"output_with_direction.mp4\",\n",
    "    cv2.VideoWriter_fourcc(*'MJPG'),  # Faster codec\n",
    "    int(cap.get(cv2.CAP_PROP_FPS)),\n",
    "    display_size\n",
    ")\n",
    "\n",
    "# === State Tracking ===\n",
    "current_ids_in_roi = set()\n",
    "unique_ids_in_roi = set()\n",
    "track_histories = defaultdict(list)\n",
    "crossed_ids = set()\n",
    "entry_crossed = {}\n",
    "exit_crossed = {}\n",
    "\n",
    "# === CSV File Setup ===\n",
    "timestamp_str = datetime.now().strftime(\"%Y%m%d_%H%M%S\")\n",
    "log_dir = \"logs\"\n",
    "os.makedirs(log_dir, exist_ok=True)\n",
    "csv_filename = os.path.join(log_dir, f\"tracking_data_{timestamp_str}.csv\")\n",
    "header = [\n",
    "    \"ID\", \"Current in ROI\", \"Passed Through ROI\", \"Crossed Entry-Exit\", \n",
    "    \"Unique People Passing from ROI\", \"Timestamp Entry\", \"Timestamp Exit\", \n",
    "    \"Duration in ROI\", \"Entry Coordinates\", \"Exit Coordinates\"\n",
    "]\n",
    "\n",
    "# Create CSV file and write header\n",
    "with open(csv_filename, mode='w', newline='') as file:\n",
    "    writer = csv.writer(file)\n",
    "    writer.writerow(header)\n",
    "\n",
    "# === Main Processing Loop ===\n",
    "frame_skip = 2  # Skip every 2nd frame to improve speed\n",
    "frame_counter = 0\n",
    "\n",
    "while cap.isOpened():\n",
    "    ret, frame = cap.read()\n",
    "    if not ret:\n",
    "        break\n",
    "\n",
    "    frame_counter += 1\n",
    "    if frame_counter % frame_skip != 0:\n",
    "        continue  # Skip processing this frame\n",
    "\n",
    "    results = model.predict(frame, conf=0.3, classes=[0], verbose=False)[0]\n",
    "    detections = []\n",
    "    xyxy = results.boxes.xyxy.cpu().numpy()\n",
    "    confs = results.boxes.conf.cpu().numpy()\n",
    "\n",
    "    for box, conf in zip(xyxy, confs):\n",
    "        x1, y1, x2, y2 = map(int, box[:4])\n",
    "        w, h = x2 - x1, y2 - y1\n",
    "\n",
    "        mask = np.zeros((height, width), dtype=np.uint8)\n",
    "        cv2.rectangle(mask, (x1, y1), (x2, y2), 1, -1)\n",
    "        overlap = np.logical_and(mask, roi_mask).sum()\n",
    "        overlap_ratio = overlap / (w * h + 1e-6)\n",
    "        if overlap_ratio > 0.1:\n",
    "            detections.append(([x1, y1, w, h], float(conf), 'person'))\n",
    "\n",
    "    tracks = tracker.update_tracks(detections, frame=frame)\n",
    "    current_ids_in_roi.clear()\n",
    "\n",
    "    for track in tracks:\n",
    "        if not track.is_confirmed():\n",
    "            continue\n",
    "\n",
    "        tid = track.track_id\n",
    "        x1, y1, x2, y2 = map(int, track.to_ltrb())\n",
    "        w, h = x2 - x1, y2 - y1\n",
    "        cx, cy = (x1 + x2) // 2, (y1 + y2) // 2\n",
    "\n",
    "        # ROI Overlap\n",
    "        mask = np.zeros((height, width), dtype=np.uint8)\n",
    "        cv2.rectangle(mask, (x1, y1), (x2, y2), 1, -1)\n",
    "        overlap = np.logical_and(mask, roi_mask).sum()\n",
    "        overlap_ratio = overlap / (w * h + 1e-6)\n",
    "        if overlap_ratio >= 0.5:\n",
    "            current_ids_in_roi.add(tid)\n",
    "            unique_ids_in_roi.add(tid)\n",
    "\n",
    "        # Tracking history\n",
    "        track_histories[tid].append((cx, cy))\n",
    "        if len(track_histories[tid]) > 2:\n",
    "            prev = track_histories[tid][-2]\n",
    "            curr = track_histories[tid][-1]\n",
    "\n",
    "            # Entry line crossing\n",
    "            sign_prev = direction_sign(*entry_line, prev)\n",
    "            sign_curr = direction_sign(*entry_line, curr)\n",
    "            if sign_prev != sign_curr and tid not in entry_crossed:\n",
    "                entry_crossed[tid] = time.time()  # Timestamp entry\n",
    "                print(f\"✅ ID {tid} crossed ENTRY line\")\n",
    "\n",
    "            # Exit line crossing\n",
    "            sign_prev = direction_sign(*exit_line, prev)\n",
    "            sign_curr = direction_sign(*exit_line, curr)\n",
    "            if sign_prev != sign_curr and tid not in exit_crossed:\n",
    "                exit_crossed[tid] = time.time()  # Timestamp exit\n",
    "                print(f\"✅ ID {tid} crossed EXIT line\")\n",
    "\n",
    "            # Count only if crossed both\n",
    "            if tid in entry_crossed and tid in exit_crossed and tid not in crossed_ids:\n",
    "                crossed_ids.add(tid)\n",
    "                print(f\"🎉 ID {tid} completed full path\")\n",
    "\n",
    "        # Draw bounding box and center pointer\n",
    "        color = (0, 255, 0) if tid in current_ids_in_roi else (0, 0, 255)\n",
    "        cv2.rectangle(frame, (x1, y1), (x2, y2), color, 2)\n",
    "        cv2.putText(frame, f\"ID:{tid}\", (x1, y1 - 10), cv2.FONT_HERSHEY_SIMPLEX, 0.6, color, 2)\n",
    "        cv2.circle(frame, (cx, cy), 4, color, -1)\n",
    "\n",
    "        # Write to CSV for each unique ID\n",
    "        if tid in crossed_ids:\n",
    "            entry_time = entry_crossed.get(tid, None)\n",
    "            exit_time = exit_crossed.get(tid, None)\n",
    "            duration_in_roi = exit_time - entry_time if entry_time and exit_time else None\n",
    "            entry_coords = entry_line[0] if entry_time else \"N/A\"\n",
    "            exit_coords = exit_line[0] if exit_time else \"N/A\"\n",
    "\n",
    "            # Save data for the ID\n",
    "            with open(csv_filename, mode='a', newline='') as file:\n",
    "                writer = csv.writer(file)\n",
    "                writer.writerow([\n",
    "                    tid, len(current_ids_in_roi), len(unique_ids_in_roi), len(crossed_ids),\n",
    "                    len(crossed_ids), entry_time, exit_time, duration_in_roi, entry_coords, exit_coords\n",
    "                ])\n",
    "\n",
    "    # Draw ROI and lines\n",
    "    cv2.polylines(frame, [roi_points], isClosed=True, color=(255, 255, 255), thickness=2)\n",
    "    cv2.line(frame, entry_line[0], entry_line[1], line_color, 2)\n",
    "    cv2.putText(frame, \"ENTRY\", entry_line[0], cv2.FONT_HERSHEY_SIMPLEX, 0.8, line_color, 2)\n",
    "    cv2.line(frame, exit_line[0], exit_line[1], line_color, 2)\n",
    "    cv2.putText(frame, \"EXIT\", exit_line[0], cv2.FONT_HERSHEY_SIMPLEX, 0.8, line_color, 2)\n",
    "\n",
    "    # Overlay stats\n",
    "    cv2.putText(frame, f\"Current People in ROI: {len(current_ids_in_roi)}\", (20, 40), cv2.FONT_HERSHEY_SIMPLEX, 1, (0, 255, 255), 2)\n",
    "    cv2.putText(frame, f\"Passing through ROI: {len(unique_ids_in_roi)}\", (20, 80), cv2.FONT_HERSHEY_SIMPLEX, 1, (255, 0, 0), 2)\n",
    "    cv2.putText(frame, f\"Crossed Entry-Exit: {len(crossed_ids)}\", (20, 120), cv2.FONT_HERSHEY_SIMPLEX, 1, (255, 0, 255), 2)\n",
    "    cv2.putText(frame, f\"Unique People Passing from ROI: {len(crossed_ids)}\", (20, 160), cv2.FONT_HERSHEY_SIMPLEX, 1, (42, 42, 165), 2)\n",
    "\n",
    "    # Show and save\n",
    "    resized = cv2.resize(frame, display_size)\n",
    "    out.write(resized)\n",
    "    cv2.imshow(\"People Tracker - Entry/Exit\", resized)\n",
    "    if cv2.waitKey(1) & 0xFF == ord('q'):\n",
    "        break\n",
    "\n",
    "cap.release()\n",
    "out.release()\n",
    "\n",
    "# === Keep window open after completion ===\n",
    "print(f\"✅ Processing complete. CSV saved as: {csv_filename}\")\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
  }
